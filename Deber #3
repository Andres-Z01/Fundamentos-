#include <iostream>

using namespace std;

int main()
{
   float Gata, Carro, llanta desinflada, llaves, llanta nueva;
   cout << "colocamos la gata debajo del carro " << endl;
   cout << "levantamos el carro con la gata " << endl;
   cout << "con la llave desaflojamos la llanta " << endl;
   cout << "cambiamos la llanta desinflada por la llanta nueva " << endl;
   cout << "aseguramos la llanta con la llave " << endl;
   cout << "al final bajamos el carro " << endl;
    return 0;
}
